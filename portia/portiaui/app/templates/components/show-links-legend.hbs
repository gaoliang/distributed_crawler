{{#tree-list}}
    {{#tree-list-item as |options|}}
        {{list-item-badge value=followedLinks color=colors.green}}
        Followed
    {{/tree-list-item}}
    {{#tree-list-item as |options|}}
        {{list-item-badge value=jsLinks color=colors.blue}}
